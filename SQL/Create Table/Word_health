
Create table innodb.world_death( 
dat date, 
death integer,
Country_Region varchar(50),
Province_State varchar(50),
PRIMARY KEY pk_death(dat),
FOREIGN KEY fk_Country(Country_Region, Province_State) REFERENCES innodb.WorldStateLocation(Country_Region, Province_State)
/*FOREIGN KEY fk_state (Province_State) REFERENCES innodb.WorldStateLocation(Province_State)*/
);

 select * from innodb.world_death;
insert into innodb.world_death
Select distinct date, sum(death), Country_Region, Province_State 
from innodb.tested_worldwide
group by date

